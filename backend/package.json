{
  "name": "groundwater-backend",
  "version": "1.0.0",
  "description": "Backend for Groundwater Data Analysis & Prediction System",
  "main": "dist/server.js",
  "type": "commonjs",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "start": "node dist/server.js",
    "build": "rimraf dist && tsc",
    "dev": "nodemon --exec ts-node src/server.ts",
    "dev:watch": "ts-node-dev --respawn --transpile-only src/server.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "echo \"No tests configured yet\" && exit 0"
  },
  "keywords": [
    "groundwater",
    "api",
    "express",
    "typescript",
    "mongodb",
    "india-wris",
    "prediction"
  ],
  "author": "Vadlamuri",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.13.2",
    "compression": "^1.7.5",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "envalid": "^8.1.1",
    "express": "^5.2.1",
    "express-rate-limit": "^7.5.1",
    "helmet": "^8.0.0",
    "hpp": "^0.2.3",
    "mongoose": "^9.1.4",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.2",
    "node-cron": "^3.0.3",
    "redis": "^5.10.0",
    "winston": "^3.19.0",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.6",
    "@types/hpp": "^0.2.7",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.10.7",
    "@types/node-cron": "^3.0.11",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "nodemon": "^3.1.9",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
